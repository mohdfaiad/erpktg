  end
  object zqrForD: TZReadOnlyQuery
    Connection = dm.zConn
    SQL.Strings = (
      'SELECT a.*, b.deskripsi FROM tbl_formula_subdet a '
      'LEFT JOIN tbl_barang b ON a.kode_brg = b.kode'
      'WHERE a.kode = :kode AND a.nama = :nama')
