  object zqrBrg: TZReadOnlyQuery
    Connection = dm.zConn
    SQL.Strings = (
      
        'SELECT kode, deskripsi, IF(COUNT(c.kode_brg)>0,1,0) f_formula, b' +
        '.stok'
      'FROM tbl_barang a '
      'LEFT JOIN tbl_barang_det b ON a.kode = b.kode_brg'
      'LEFT JOIN tbl_formula_head c ON a.kode = c.kode_brg'
      'WHERE b.kode_gdg = '#39'G01'#39' '
      'GROUP BY kode, deskripsi, b.stok'
      'ORDER BY deskripsi')
    Params = <>
    Left = 425
    Top = 90
  end
  object dsBrg: TDataSource
    DataSet = zqrBrg
    Left = 455
    Top = 90
  end
  object zqrForH: TZReadOnlyQuery
    Connection = dm.zConn
    SQL.Strings = (
      'SELECT a.*, b.deskripsi FROM tbl_formula_det a '
      'LEFT JOIN tbl_barang b ON a.kode = b.kode'
      'WHERE a.kode = :kode')
    Params = <
