  end
  object zRencana: TZQuery
    Connection = dm.zConn
    SQL.Strings = (
      
        'SELECT  a.*,b.nama,ifnull(c.nama,a.nama) sales  FROM tbl_rencana' +
        '_kerja as a left join tbl_customer as b'
      
        'on a.cust=b.kode left join tbl_sales c on a.nama = c.kode_sales ' +
        'order by a.tgl desc')
    Params = <>
    Left = 443
    Top = 17
  end
